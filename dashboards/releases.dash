[dashboard]
title = Releases Inbox
description = Releases
foreach = is:open

[section "mitaka"]
query = project:openstack/releases file:^deliverables/mitaka/.*

[section "liberty"]
query = project:openstack/releases file:^deliverables/liberty/.*

[section "kilo"]
query = project:openstack/releases file:^deliverables/kilo/.*

[section "juno"]
query = project:openstack/releases file:^deliverables/juno/.*

[section "independent"]
query = project:openstack/releases file:^deliverables/_independent/.*

[section "all releases"]
query = is:open project:openstack/releases

[section "tools"]
query = project:openstack-infra/release-tools

[section "reno"]
query = project:openstack/reno

[section "release-test"]
query = project:openstack-infra/release-test
